<div id="content-header">
  <header>
    <h1>New Event Question</h1>
  </header>
</div>

<div id="content-navigation">
  <%= render_breadcrumbs @conn %>
  <%= render_presence @conn %>
</div>

<div id="content">
  <%= render_flash_notifications @conn %>

  <section>
    <%= render "form.html", Map.put(assigns, :action, Routes.event_question_path(@conn, :create)) %>
  </section>
</div>
