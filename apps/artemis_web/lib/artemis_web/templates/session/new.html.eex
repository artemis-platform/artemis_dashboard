<main role="main" id="login">
  <section>
    <h1>
      <i class="fire icon"></i>
      Artemis Dashboard
    </h1>
    <ul class="providers">
      <%= for provider <- @providers do %>
        <li>
          <%= action provider.title, to: provider.link, color: get_provider_color(provider), size: "medium" %>
        </li>
      <% end %>
    </ul>
  </section>
</main>
